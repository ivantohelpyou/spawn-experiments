{
  "application": {
    "name": "ConfigParser Demo",
    "version": "1.0.0",
    "debug": true,
    "port": 8080
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "configdb",
    "ssl": false,
    "timeout": 30
  },
  "logging": {
    "level": "INFO",
    "file": "/var/log/app.log",
    "rotate": true
  },
  "features": [
    "authentication",
    "api",
    "web_interface",
    "monitoring"
  ],
  "cache": {
    "type": "redis",
    "ttl": 3600,
    "cluster_nodes": [
      "redis-1:6379",
      "redis-2:6379",
      "redis-3:6379"
    ]
  }
}